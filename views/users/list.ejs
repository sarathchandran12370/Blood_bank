<table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Phone</th>
        <th scope="col">place</th>
      </tr>
    </thead>
    <tbody>
    <% for(var i=0; i < data.length; i++) { %>
    
      <tr>
        <th scope="row"><%=i+1%></th>
        <td><%=data[i].name%></td>
        <td><%=data[i].phone%></td>
        <td><%=data[i].place%></td>
      </tr>
      <% } %>
    </tbody>
  </table>